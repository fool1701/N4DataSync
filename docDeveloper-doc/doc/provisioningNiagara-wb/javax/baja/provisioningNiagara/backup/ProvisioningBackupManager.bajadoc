<!-- javax.baja.provisioningNiagara.backup.ProvisioningBackupManager.listBackupStepDetails(javax.baja.driver.BDevice) -->
<method name="listBackupStepDetails"  public="true" abstract="true">
<description>
List all available details for backups that have been performed by provisioning&#xa; jobs against the given Niagara station
</description>
<parameter name="station">
<type class="javax.baja.driver.BDevice"/>
<description>
a station object in a supervisor&#x27;s Niagara Network that is&#xa; managed with provisioning
</description>
</parameter>
<return>
<type class="javax.baja.batchJob.driver.BDeviceStepDetails" dimension="1"/>
</return>
<throws>
<type class="java.lang.Exception"/>
</throws>
</method>

<!-- javax.baja.provisioningNiagara.backup.ProvisioningBackupManager.getBackupFileOrd(javax.baja.batchJob.driver.BDeviceStepDetails) -->
<method name="getBackupFileOrd"  public="true" abstract="true">
<description>
Return the Ord for the distribution file that was created by the backup&#xa; step described by the given details object.
</description>
<parameter name="backupStepDetails">
<type class="javax.baja.batchJob.driver.BDeviceStepDetails"/>
<description>
details object from a station backup step
</description>
</parameter>
<return>
<type class="javax.baja.naming.BOrd"/>
<description>
Ord for a distribution file
</description>
</return>
<throws>
<type class="java.lang.Exception"/>
</throws>
</method>

<!-- javax.baja.provisioningNiagara.backup.ProvisioningBackupManager.startRestoreJob(javax.baja.batchJob.driver.BDeviceStepDetails) -->
<method name="startRestoreJob"  public="true" abstract="true">
<description>
Start a job on the station&#x27;s supervisor which will install the backup&#xa; distribution file to the station.
</description>
<parameter name="backupStepDetails">
<type class="javax.baja.batchJob.driver.BDeviceStepDetails"/>
<description>
details object from a station backup step
</description>
</parameter>
<return>
<type class="javax.baja.naming.BOrd"/>
<description>
Ord for the batch job that was submitted to perform the restore
</description>
</return>
<throws>
<type class="java.lang.Exception"/>
</throws>
</method>

