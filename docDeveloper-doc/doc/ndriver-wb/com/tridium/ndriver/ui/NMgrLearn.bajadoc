<constructor name="NMgrLearn" public="true">
<parameter name="manager">
<type class="javax.baja.workbench.mgr.BAbstractManager"/>
</parameter>
<description/>
</constructor>

<!-- com.tridium.ndriver.ui.NMgrLearn.setJob(javax.baja.job.BJob) -->
<method name="setJob"  public="true">
<annotation><type class="java.lang.Override"/>
</annotation>
<description>
Override to add subscriber to update table as leafs are added or removed
</description>
<parameter name="job">
<type class="javax.baja.job.BJob"/>
</parameter>
<return>
<type class="void"/>
</return>
</method>

<!-- com.tridium.ndriver.ui.NMgrLearn.makeColumns() -->
<method name="makeColumns"  protected="true">
<annotation><type class="java.lang.Override"/>
</annotation>
<description>
Automatically determines the MgrColumns for the discovery list.
</description>
<return>
<type class="javax.baja.workbench.mgr.MgrColumn" dimension="1"/>
</return>
</method>

<!-- com.tridium.ndriver.ui.NMgrLearn.toRow(java.lang.Object, javax.baja.workbench.mgr.MgrEditRow) -->
<method name="toRow"  public="true">
<annotation><type class="java.lang.Override"/>
</annotation>
<description>
Automatically synchronizes the data in a discovery object with an instance&#xa; of MgrEditRow representing a database object to be modified.
</description>
<parameter name="discovery">
<type class="java.lang.Object"/>
</parameter>
<parameter name="row">
<type class="javax.baja.workbench.mgr.MgrEditRow"/>
</parameter>
<return>
<type class="void"/>
</return>
</method>

<!-- com.tridium.ndriver.ui.NMgrLearn.jobComplete(javax.baja.job.BJob) -->
<method name="jobComplete"  public="true">
<annotation><type class="java.lang.Override"/>
</annotation>
<description>
This callback is automatically invoked when the current job set via&#xa; &lt;code&gt;setJob()&lt;/code&gt; completes.
</description>
<parameter name="job">
<type class="javax.baja.job.BJob"/>
</parameter>
<return>
<type class="void"/>
</return>
</method>

<!-- com.tridium.ndriver.ui.NMgrLearn.isDepthExpandable(int) -->
<method name="isDepthExpandable"  public="true">
<annotation><type class="java.lang.Override"/>
</annotation>
<description>
All depths are potentially expandable.
</description>
<parameter name="depth">
<type class="int"/>
</parameter>
<return>
<type class="boolean"/>
</return>
</method>

