<constructor name="BPxProfile" public="true">
<parameter name="editor">
<type class="javax.baja.px.editor.BPxEditor"/>
</parameter>
<description/>
</constructor>

<!-- javax.baja.px.editor.BPxProfile.getType() -->
<method name="getType"  public="true">
<annotation><type class="java.lang.Override"/>
</annotation>
<description/>
<return>
<type class="javax.baja.sys.Type"/>
</return>
</method>

<!-- javax.baja.px.editor.BPxProfile.getSideBars(javax.baja.px.editor.BPxSideBar[]) -->
<method name="getSideBars"  public="true">
<description/>
<parameter name="defaultSideBars">
<type class="javax.baja.px.editor.BPxSideBar" dimension="1"/>
<description>
the sidebars ordinarily used by the PxEditor.
</description>
</parameter>
<return>
<type class="javax.baja.px.editor.BPxSideBar" dimension="1"/>
<description>
the Px sidebars to use in the PxEditor, or an empty array for no sidebars.&#xa; The default is to return &lt;code&gt;defaultSideBars&lt;/code&gt;.
</description>
</return>
</method>

<!-- javax.baja.px.editor.BPxProfile.getViewMenus(javax.baja.ui.BMenu[]) -->
<method name="getViewMenus"  public="true">
<description>
Get an array of BMenus that the PxEditor exposes.&#xa; These menus will be merged with the application&#xa; menus when the PxEditor is the active plugin in&#xa; its plugin context.
</description>
<parameter name="defaultMenus">
<type class="javax.baja.ui.BMenu" dimension="1"/>
<description>
the menus ordinarily used by the PxEditor.
</description>
</parameter>
<return>
<type class="javax.baja.ui.BMenu" dimension="1"/>
<description>
array of menus to merge, or an empty array if no menus are required.&#xa; The default is to return &lt;code&gt;defaultMenus&lt;/code&gt;.
</description>
</return>
</method>

<!-- javax.baja.px.editor.BPxProfile.getViewToolBar(javax.baja.ui.BToolBar) -->
<method name="getViewToolBar"  public="true">
<description>
Get the toolbar that the PxEditor exposes.  This toolbar&#xa; will be merged with the application toolbar when the PxEditor &#xa; is the active plugin in its plugin context.
</description>
<parameter name="defaultToolBar">
<type class="javax.baja.ui.BToolBar"/>
<description>
the toolBar ordinarily used by the PxEditor.
</description>
</parameter>
<return>
<type class="javax.baja.ui.BToolBar"/>
<description>
BToolBar to merge, or null if no toolbar is required.&#xa; The default is to return &lt;code&gt;defaultToolBar&lt;/code&gt;.
</description>
</return>
</method>

<!-- javax.baja.px.editor.BPxProfile.getViewStatusBarSupplement(javax.baja.ui.BWidget) -->
<method name="getViewStatusBarSupplement"  public="true">
<description>
Get a BWidget to display to the right of the status bar.&#xa; If there is no status bar in the shell then this method&#xa; is ignored.  The widget returned is displayed to the right&#xa; of the normal status bar and should have a preferred width&#xa; no wider than necessary.  The preferred height of the widget&#xa; is ignored and forced to the height of the normal status &#xa; bar.
</description>
<parameter name="defaultSupplement">
<type class="javax.baja.ui.BWidget"/>
<description>
the status bar supplement ordinarily used by the PxEditor.
</description>
</parameter>
<return>
<type class="javax.baja.ui.BWidget"/>
<description>
BWidget to append to the normal status bar, &#xa; or null if no supplement is required.&#xa; The default is to return &lt;code&gt;defaultSupplement&lt;/code&gt;.
</description>
</return>
</method>

<!-- javax.baja.px.editor.BPxProfile.getPxEditor() -->
<method name="getPxEditor"  public="true">
<description/>
<return>
<type class="javax.baja.px.editor.BPxEditor"/>
</return>
</method>

