<class module="pxEditor" runtimeProfile="wb" qualifiedName="javax.baja.px.editor.factory.WidgetInserter" name="WidgetInserter" packageName="javax.baja.px.editor.factory" public="true">
<description>
WidgetInserter contains information on how to insert widgets into the PxEditor.
</description>
<tag name="@author">Mike Jarmy</tag>
<tag name="@creation">16 Aug 07</tag>
<tag name="@version">$Revision: 4$ $Date: 1/9/08 8:49:51 AM EST$</tag>
<tag name="@since">Baja 1.0</tag>
<extends>
<type class="java.lang.Object"/>
</extends>
<!-- javax.baja.px.editor.factory.WidgetInserter(javax.baja.ui.BWidget[], javax.baja.ui.Command, int) -->
<constructor name="WidgetInserter" public="true">
<parameter name="widgets">
<type class="javax.baja.ui.BWidget" dimension="1"/>
<description>
The widgets to be inserted.
</description>
</parameter>
<parameter name="auxCommand">
<type class="javax.baja.ui.Command"/>
<description>
An auxillary command that should be&#xa; executed when the widgets are inserted.  This parameter can generally&#xa; be left null, but it is useful for doing extra work like connecting&#xa; widget properties to PxProperties.
</description>
</parameter>
<parameter name="columnCount">
<type class="int"/>
<description>
the number of columns to use when laying out the widgets&#xa; in a canvas pane.  This is usually &#x27;1&#x27;, but can be larger when laying out widgets&#xa; in a grid.
</description>
</parameter>
<description>
Construct a WidgetInserter.
</description>
</constructor>

<!-- javax.baja.px.editor.factory.WidgetInserter(javax.baja.ui.BWidget[], javax.baja.ui.Command) -->
<constructor name="WidgetInserter" public="true">
<parameter name="widgets">
<type class="javax.baja.ui.BWidget" dimension="1"/>
</parameter>
<parameter name="auxCommand">
<type class="javax.baja.ui.Command"/>
</parameter>
<description>
Convenience for &lt;code&gt;new WidgetInserter(widgets, auxCommand, false, 1)&lt;/code&gt;.
</description>
</constructor>

<!-- javax.baja.px.editor.factory.WidgetInserter(javax.baja.ui.BWidget[]) -->
<constructor name="WidgetInserter" public="true">
<parameter name="widgets">
<type class="javax.baja.ui.BWidget" dimension="1"/>
</parameter>
<description>
Convenience for &lt;code&gt;new WidgetInserter(widgets, null, 1)&lt;/code&gt;.
</description>
</constructor>

<!-- javax.baja.px.editor.factory.WidgetInserter.getWidgets() -->
<method name="getWidgets"  public="true">
<description>
The widgets to be inserted.
</description>
<return>
<type class="javax.baja.ui.BWidget" dimension="1"/>
</return>
</method>

<!-- javax.baja.px.editor.factory.WidgetInserter.getAuxillaryCommand() -->
<method name="getAuxillaryCommand"  public="true">
<description>
The auxillary command to be executed when the widgets are inserted.
</description>
<return>
<type class="javax.baja.ui.Command"/>
</return>
</method>

<!-- javax.baja.px.editor.factory.WidgetInserter.getColumnCount() -->
<method name="getColumnCount"  public="true">
<description>
The number of columns in each row of the grid of widgets. &#xa; This method is only called when the widgets are being dropped onto a canvas pane.
</description>
<return>
<type class="int"/>
</return>
</method>

</class>
