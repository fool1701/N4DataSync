<class module="pxEditor" runtimeProfile="wb" qualifiedName="javax.baja.px.editor.PxEditorSelection" name="PxEditorSelection" packageName="javax.baja.px.editor" public="true" interface="true" abstract="true" category="interface">
<description>
PxEditorSelection
</description>
<tag name="@author">Mike Jarmy</tag>
<tag name="@creation">16 Aug 07</tag>
<tag name="@version">$Revision: 2$ $Date: 9/17/07 8:50:33 AM EDT$</tag>
<tag name="@since">Baja 1.0</tag>
<!-- javax.baja.px.editor.PxEditorSelection.getWidgets() -->
<method name="getWidgets"  public="true" abstract="true">
<description>
Get the list of selected widgets.  If no widgets are selected, &#xa; an empty array is returned.
</description>
<return>
<type class="javax.baja.ui.BWidget" dimension="1"/>
</return>
</method>

<!-- javax.baja.px.editor.PxEditorSelection.setWidgets(javax.baja.ui.BWidget[]) -->
<method name="setWidgets"  public="true" abstract="true">
<description>
Deselect all selected widgets, and then add the specified widgets &#xa; to the list of selected widgets.
</description>
<parameter name="widgets">
<type class="javax.baja.ui.BWidget" dimension="1"/>
</parameter>
<return>
<type class="void"/>
</return>
</method>

<!-- javax.baja.px.editor.PxEditorSelection.select(javax.baja.ui.BWidget) -->
<method name="select"  public="true" abstract="true">
<description>
Add the specified widget to the list of selected widgets.
</description>
<parameter name="widget">
<type class="javax.baja.ui.BWidget"/>
</parameter>
<return>
<type class="void"/>
</return>
</method>

<!-- javax.baja.px.editor.PxEditorSelection.deselect(javax.baja.ui.BWidget) -->
<method name="deselect"  public="true" abstract="true">
<description>
Remove the specified widget from the list of selected widgets.
</description>
<parameter name="widget">
<type class="javax.baja.ui.BWidget"/>
</parameter>
<return>
<type class="void"/>
</return>
</method>

<!-- javax.baja.px.editor.PxEditorSelection.deselectAll() -->
<method name="deselectAll"  public="true" abstract="true">
<description>
Deselect all selected widgets.
</description>
<return>
<type class="void"/>
</return>
</method>

<!-- javax.baja.px.editor.PxEditorSelection.isSelected(javax.baja.ui.BWidget) -->
<method name="isSelected"  public="true" abstract="true">
<description>
Return whether the specified widget is currently selected.
</description>
<parameter name="widget">
<type class="javax.baja.ui.BWidget"/>
</parameter>
<return>
<type class="boolean"/>
</return>
</method>

</class>
</bajadoc>
