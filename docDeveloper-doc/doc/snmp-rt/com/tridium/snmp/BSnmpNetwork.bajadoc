</property>

<property name="enterprise" flags="r">
<type class="java.lang.String"/>
<description>
Slot for the &lt;code&gt;enterprise&lt;/code&gt; property.&#xa; The enterprise of this Niagara station.
</description>
<tag name="@see">#getEnterprise</tag>
<tag name="@see">#setEnterprise</tag>
</property>

<property name="contact" flags="">
<type class="java.lang.String"/>
<description>
Slot for the &lt;code&gt;contact&lt;/code&gt; property.&#xa; The contact of this Niagara station
</description>
<tag name="@see">#getContact</tag>
<tag name="@see">#setContact</tag>
</property>

<property name="systemName" flags="">
<type class="java.lang.String"/>
<description>
Slot for the &lt;code&gt;systemName&lt;/code&gt; property.&#xa; The name of this Niagara station
</description>
<tag name="@see">#getSystemName</tag>
<tag name="@see">#setSystemName</tag>
</property>

<property name="location" flags="">
<type class="java.lang.String"/>
<description>
Slot for the &lt;code&gt;location&lt;/code&gt; property.&#xa; The location of this Niagara station
</description>
<tag name="@see">#getLocation</tag>
<tag name="@see">#setLocation</tag>
</property>

<property name="snmpReceiveRequests" flags="">
<type class="boolean"/>
<description>
Slot for the &lt;code&gt;snmpReceiveRequests&lt;/code&gt; property.&#xa; enables or disables the receiving and processing&#xa; of SNMP requests from outside sources.
</description>
<tag name="@see">#getSnmpReceiveRequests</tag>
<tag name="@see">#setSnmpReceiveRequests</tag>
</property>

<property name="snmpReceivePort" flags="">
<type class="javax.baja.firewall.BServerPort"/>
<description>
Slot for the &lt;code&gt;snmpReceivePort&lt;/code&gt; property.&#xa; The local port to use for receiving SNMP&#xa; requests from outside sources.
</description>
<tag name="@see">#getSnmpReceivePort</tag>
<tag name="@see">#setSnmpReceivePort</tag>
</property>

<property name="ignoreRequestsFromUnrecognizedSources" flags="">
<type class="boolean"/>
<description>
Slot for the &lt;code&gt;ignoreRequestsFromUnrecognizedSources&lt;/code&gt; property.&#xa; enables or disables the receiving and processing&#xa; SNMP requests only when the source of the request&#xa; is from a recognized source.
</description>
<tag name="@see">#getIgnoreRequestsFromUnrecognizedSources</tag>
<tag name="@see">#setIgnoreRequestsFromUnrecognizedSources</tag>
</property>

<property name="recognizedSources" flags="">
<type class="com.tridium.snmp.datatypes.BNetworkManagerList"/>
<description>
Slot for the &lt;code&gt;recognizedSources&lt;/code&gt; property.&#xa; contains the list of source IPs that&#xa; are used to verify if a received request is&#xa; from a known source.
</description>
<tag name="@see">#getRecognizedSources</tag>
<tag name="@see">#setRecognizedSources</tag>
</property>

<property name="checkCommunityOnRequests" flags="">
<type class="boolean"/>
<description>
Slot for the &lt;code&gt;checkCommunityOnRequests&lt;/code&gt; property.&#xa; enables or disables checking the community field&#xa; on received SNMP requests before processing the&#xa; request.  If enabled and the community field does&#xa; not match what is expected, the request will not&#xa; be processed.
</description>
<tag name="@see">#getCheckCommunityOnRequests</tag>
<tag name="@see">#setCheckCommunityOnRequests</tag>
</property>

<property name="readOnlyCommunity" flags="">
<type class="java.lang.String"/>
<description>
Slot for the &lt;code&gt;readOnlyCommunity&lt;/code&gt; property.&#xa; The community expected on received SNMP requests&#xa; to read data.
</description>
<tag name="@see">#getReadOnlyCommunity</tag>
<tag name="@see">#setReadOnlyCommunity</tag>
</property>

<property name="readWriteCommunity" flags="">
<type class="java.lang.String"/>
<description>
Slot for the &lt;code&gt;readWriteCommunity&lt;/code&gt; property.&#xa; The community expected on received SNMP requests&#xa; to read or write data.
</description>
<tag name="@see">#getReadWriteCommunity</tag>
<tag name="@see">#setReadWriteCommunity</tag>
</property>

<property name="snmpReceiveTraps" flags="">
<type class="boolean"/>
<description>
Slot for the &lt;code&gt;snmpReceiveTraps&lt;/code&gt; property.&#xa; enables or disables the receiving and processing&#xa; of SNMP traps from outside sources.
</description>
<tag name="@see">#getSnmpReceiveTraps</tag>
<tag name="@see">#setSnmpReceiveTraps</tag>
</property>

<property name="snmpReceiveTrapsPort" flags="">
<type class="javax.baja.firewall.BServerPort"/>
<description>
Slot for the &lt;code&gt;snmpReceiveTrapsPort&lt;/code&gt; property.&#xa; The local port to use for receiving SNMP&#xa; traps from outside sources.
</description>
<tag name="@see">#getSnmpReceiveTrapsPort</tag>
<tag name="@see">#setSnmpReceiveTrapsPort</tag>
</property>

<property name="defaultNetworkManagerIpAddress" flags="">
<type class="java.lang.String"/>
<description>
Slot for the &lt;code&gt;defaultNetworkManagerIpAddress&lt;/code&gt; property.&#xa; The IP address of the outside network manager.&#xa; (i.e. specifies where to send outgoing&#xa; trap messages generated by Niagara)
</description>
<tag name="@see">#getDefaultNetworkManagerIpAddress</tag>
<tag name="@see">#setDefaultNetworkManagerIpAddress</tag>
</property>

<property name="defaultNetworkManagerTrapsPort" flags="">
<type class="int"/>
<description>
Slot for the &lt;code&gt;defaultNetworkManagerTrapsPort&lt;/code&gt; property.&#xa; The remote port on the network manager&#xa; where outgoing traps from this station&#xa; are sent.
</description>
<tag name="@see">#getDefaultNetworkManagerTrapsPort</tag>
<tag name="@see">#setDefaultNetworkManagerTrapsPort</tag>
</property>

<property name="defaultNetworkManagerSnmpVersion" flags="">
<type class="int"/>
<description>
Slot for the &lt;code&gt;defaultNetworkManagerSnmpVersion&lt;/code&gt; property.&#xa; SNMP version supported by network manager.
</description>
<tag name="@see">#getDefaultNetworkManagerSnmpVersion</tag>
<tag name="@see">#setDefaultNetworkManagerSnmpVersion</tag>
</property>

<property name="defaultNetworkManagerTrapsCommunity" flags="">
<type class="java.lang.String"/>
<description>
Slot for the &lt;code&gt;defaultNetworkManagerTrapsCommunity&lt;/code&gt; property.&#xa; The community for SNMP trap messages&#xa; expected by the network manager.
</description>
<tag name="@see">#getDefaultNetworkManagerTrapsCommunity</tag>
<tag name="@see">#setDefaultNetworkManagerTrapsCommunity</tag>
</property>

<property name="snmpAlarmTableCapacity" flags="">
<type class="int"/>
<description>
Slot for the &lt;code&gt;snmpAlarmTableCapacity&lt;/code&gt; property.&#xa; The size of the cache of alarm records stored&#xa; in the snmp alarm table.  When full, the snmp&#xa; alarm table will roll by default.  A size of&#xa; zero disables the snmp alarm table altogether.
</description>
<tag name="@see">#getSnmpAlarmTableCapacity</tag>
<tag name="@see">#setSnmpAlarmTableCapacity</tag>
</property>

<property name="localDevice" flags="">
<type class="com.tridium.snmp.BSnmpAgent"/>
<description>
Slot for the &lt;code&gt;localDevice&lt;/code&gt; property.&#xa; the representation of this Niagara station as a SNMP device.
</description>
<tag name="@see">#getLocalDevice</tag>
<tag name="@see">#setLocalDevice</tag>
</property>

<action name="clearSnmpAlarmTable" flags="ac">
<return>
<type class="void"/>
</return>
<description>
Slot for the &lt;code&gt;clearSnmpAlarmTable&lt;/code&gt; action.&#xa; Clears the cache of alarm records stored in the&#xa; snmp alarm table
</description>
<tag name="@see">#clearSnmpAlarmTable()</tag>
</action>

