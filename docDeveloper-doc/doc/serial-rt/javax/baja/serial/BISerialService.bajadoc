<type class="java.lang.String"/>
</parameter>
<return>
<type class="java.lang.String"/>
</return>
<throws>
<type class="javax.baja.serial.PortNotFoundException"/>
</throws>
</method>

<!-- javax.baja.serial.BISerialService.openPort(java.lang.String, java.lang.String) -->
<method name="openPort"  public="true" abstract="true">
<description>
Open port for use by the given owner.&#xa;  The port may be released by calling BSerialPort.close();
</description>
<parameter name="port">
<type class="java.lang.String"/>
</parameter>
<parameter name="owner">
<type class="java.lang.String"/>
</parameter>
