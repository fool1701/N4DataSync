<class module="fox" runtimeProfile="rt" qualifiedName="com.tridium.fox.sys.BFoxService" name="BFoxService" packageName="com.tridium.fox.sys" public="true">
<description>
BFoxService is the Baja component wrapper for the FoxServer daemon.&#xa; It is commonly used within a NiagaraNetwork, but can be used&#xa; stand alone to provide basic Fox accessibility.
</description>
<tag name="@author">Brian Frank</tag>
<tag name="@version">$Revision: 60$ $Date: 7/13/11 3:13:19 PM EDT$</tag>
<tag name="@creation">8 May 02</tag>
<tag name="@since">Baja 1.0</tag>
<extends>
<type class="javax.baja.sys.BComponent"/>
</extends>
<implements>
<type class="javax.baja.sys.BIService"/>
</implements>
<implements>
<type class="javax.baja.util.BIRestrictedComponent"/>
</implements>
<implements>
<type class="com.tridium.security.BISecurityInfoSource"/>
</implements>
<implements>
<type class="com.tridium.crypto.core.io.TrustAnchorConsumer"/>
</implements>
<property name="foxPort" flags="">
<type class="javax.baja.firewall.BServerPort"/>
<description>
Slot for the &lt;code&gt;foxPort&lt;/code&gt; property.&#xa; Well known port for fox server, default is 1911.
</description>
<tag name="@see">#getFoxPort</tag>
<tag name="@see">#setFoxPort</tag>
</property>

<property name="foxEnabled" flags="">
<type class="boolean"/>
<description>
Slot for the &lt;code&gt;foxEnabled&lt;/code&gt; property.&#xa; Whether or not to process fox requests.  If false,&#xa; and foxs is enabled, the response will redirect to&#xa; to foxs .
</description>
<tag name="@see">#getFoxEnabled</tag>
<tag name="@see">#setFoxEnabled</tag>
</property>

<property name="foxsPort" flags="">
<type class="javax.baja.firewall.BServerPort"/>
<description>
Slot for the &lt;code&gt;foxsPort&lt;/code&gt; property.&#xa; Port that the foxs server listens on.
</description>
<tag name="@see">#getFoxsPort</tag>
<tag name="@see">#setFoxsPort</tag>
</property>

<property name="foxsEnabled" flags="">
<type class="boolean"/>
<description>
Slot for the &lt;code&gt;foxsEnabled&lt;/code&gt; property.&#xa; Whether or not to process foxs requests.
</description>
<tag name="@see">#getFoxsEnabled</tag>
<tag name="@see">#setFoxsEnabled</tag>
</property>

<property name="foxsOnly" flags="">
<type class="boolean"/>
<description>
Slot for the &lt;code&gt;foxsOnly&lt;/code&gt; property.&#xa; Force requests to the http server as&#xa; redirects to the foxs port.
</description>
<tag name="@see">#getFoxsOnly</tag>
<tag name="@see">#setFoxsOnly</tag>
</property>

<property name="foxsMinProtocol" flags="">
<type class="javax.baja.security.crypto.BSslTlsEnum"/>
<description>
Slot for the &lt;code&gt;foxsMinProtocol&lt;/code&gt; property.&#xa; select the minimum security protocol
</description>
<tag name="@see">#getFoxsMinProtocol</tag>
<tag name="@see">#setFoxsMinProtocol</tag>
</property>

<property name="cipherSuiteGroup" flags="">
<type class="javax.baja.security.crypto.BTlsCipherSuiteGroup"/>
<description>
Slot for the &lt;code&gt;cipherSuiteGroup&lt;/code&gt; property.&#xa; The cipher suite group to use for the tls connections.
</description>
<tag name="@see">#getCipherSuiteGroup</tag>
<tag name="@see">#setCipherSuiteGroup</tag>
</property>

<property name="foxsCert" flags="">
<type class="java.lang.String"/>
<description>
Slot for the &lt;code&gt;foxsCert&lt;/code&gt; property.&#xa; The alias of the cert to use. This property is no longer used and migrates to the&#xa; certAliasAndPassword slot starting with Niagara 4.13
</description>
<tag name="@see">#getFoxsCert</tag>
<tag name="@see">#setFoxsCert</tag>
</property>

<property name="certAliasAndPassword" flags="h">
<type class="javax.baja.security.BCertificateAliasAndPassword"/>
<description>
Slot for the &lt;code&gt;certAliasAndPassword&lt;/code&gt; property.&#xa; The certificate and optional password to use for tls connections.
</description>
<tag name="@since">Niagara 4.13</tag>
<tag name="@see">#getCertAliasAndPassword</tag>
<tag name="@see">#setCertAliasAndPassword</tag>
</property>

<property name="serverCertificateHealth" flags="htr">
<type class="com.tridium.security.BServerCertificateHealth"/>
<description>
Slot for the &lt;code&gt;serverCertificateHealth&lt;/code&gt; property.&#xa; Provide a status regarding the cert in case it can&#x27;t use the configured one
</description>
<tag name="@since">Niagara 4.13</tag>
<tag name="@see">#getServerCertificateHealth</tag>
<tag name="@see">#setServerCertificateHealth</tag>
</property>

<property name="requestTimeout" flags="">
<type class="javax.baja.sys.BRelTime"/>
<description>
Slot for the &lt;code&gt;requestTimeout&lt;/code&gt; property.&#xa; Time to wait for a response for assuming the&#xa; connection is dead.
</description>
<tag name="@see">#getRequestTimeout</tag>
<tag name="@see">#setRequestTimeout</tag>
</property>

<property name="socketOptionTimeout" flags="">
<type class="javax.baja.sys.BRelTime"/>
<description>
Slot for the &lt;code&gt;socketOptionTimeout&lt;/code&gt; property.&#xa; Time to wait on a socket read before assuming&#xa; the connection is dead.
</description>
<tag name="@see">#getSocketOptionTimeout</tag>
<tag name="@see">#setSocketOptionTimeout</tag>
</property>

<property name="socketTcpNoDelay" flags="">
<type class="boolean"/>
<description>
Slot for the &lt;code&gt;socketTcpNoDelay&lt;/code&gt; property.&#xa; Enables or disables a socket&#x27;s TCP_NODELAY setting&#xa; (disables or enables Nagle&#x27;s algorithm for the socket).
</description>
<tag name="@since">Niagara 3.3</tag>
<tag name="@see">#getSocketTcpNoDelay</tag>
<tag name="@see">#setSocketTcpNoDelay</tag>
</property>

<property name="keepAliveInterval" flags="">
<type class="javax.baja.sys.BRelTime"/>
<description>
Slot for the &lt;code&gt;keepAliveInterval&lt;/code&gt; property.&#xa; Interval between keep alive messages.  The keep&#xa; alive should be well below the requestTimeout&#xa; and socketOptionTimeout.
</description>
<tag name="@see">#getKeepAliveInterval</tag>
<tag name="@see">#setKeepAliveInterval</tag>
</property>

<property name="maxServerSessions" flags="">
<type class="int"/>
<description>
Slot for the &lt;code&gt;maxServerSessions&lt;/code&gt; property.&#xa; Max number of server connections before clients&#xa; connections error with busy.
</description>
<tag name="@see">#getMaxServerSessions</tag>
<tag name="@see">#setMaxServerSessions</tag>
</property>

<property name="multicastEnabled" flags="">
<type class="boolean"/>
<description>
Slot for the &lt;code&gt;multicastEnabled&lt;/code&gt; property.&#xa; Whether or not to allow multicast broadcasts/responses.&#xa; Multicast is the mechanism used for station discovery, either to initiate&#xa; a broadcast to discover Niagara stations, or to respond to a broadcast&#xa; with an announcement response.  Disabling this property will turn off&#xa; both.
</description>
<tag name="@since">Niagara 3.7</tag>
<tag name="@see">#getMulticastEnabled</tag>
<tag name="@see">#setMulticastEnabled</tag>
</property>

<property name="enableAnnouncement" flags="">
<type class="boolean"/>
<description>
Slot for the &lt;code&gt;enableAnnouncement&lt;/code&gt; property.&#xa; Enable the multicast announcement messages used for learn.
</description>
<tag name="@see">#getEnableAnnouncement</tag>
<tag name="@see">#setEnableAnnouncement</tag>
</property>

<property name="multicastTimeToLive" flags="">
<type class="int"/>
<description>
Slot for the &lt;code&gt;multicastTimeToLive&lt;/code&gt; property.&#xa; Number of hops to make before a multicast message expires.
</description>
<tag name="@see">#getMulticastTimeToLive</tag>
<tag name="@see">#setMulticastTimeToLive</tag>
</property>

<property name="serverConnections" flags="">
<type class="com.tridium.fox.sys.BServerConnections"/>
<description>
Slot for the &lt;code&gt;serverConnections&lt;/code&gt; property.&#xa; Stores all server connections.
</description>
<tag name="@see">#getServerConnections</tag>
<tag name="@see">#setServerConnections</tag>
</property>

<property name="traceSessionStates" flags="">
<type class="boolean"/>
<description>
Slot for the &lt;code&gt;traceSessionStates&lt;/code&gt; property.&#xa; Trace session state changes.
</description>
<tag name="@see">#getTraceSessionStates</tag>
<tag name="@see">#setTraceSessionStates</tag>
</property>

<property name="traceReadFrame" flags="">
<type class="boolean"/>
<description>
Slot for the &lt;code&gt;traceReadFrame&lt;/code&gt; property.&#xa; Dump frames being read from wire.
</description>
<tag name="@see">#getTraceReadFrame</tag>
<tag name="@see">#setTraceReadFrame</tag>
</property>

<property name="traceWriteFrame" flags="">
<type class="boolean"/>
<description>
Slot for the &lt;code&gt;traceWriteFrame&lt;/code&gt; property.&#xa; Dump frames being written from wire.
</description>
<tag name="@see">#getTraceWriteFrame</tag>
<tag name="@see">#setTraceWriteFrame</tag>
</property>

<property name="traceMulticast" flags="">
<type class="boolean"/>
<description>
Slot for the &lt;code&gt;traceMulticast&lt;/code&gt; property.&#xa; Trace multicast messaging.
</description>
<tag name="@see">#getTraceMulticast</tag>
<tag name="@see">#setTraceMulticast</tag>
</property>

<property name="auditStationLoginEvents" flags="">
<type class="boolean"/>
<description>
Slot for the &lt;code&gt;auditStationLoginEvents&lt;/code&gt; property.&#xa; If true, login and logout events for station connections will be logged&#xa; by the system auditor
</description>
<tag name="@since">Niagara 4.1</tag>
<tag name="@see">#getAuditStationLoginEvents</tag>
<tag name="@see">#setAuditStationLoginEvents</tag>
</property>

<property name="supportLegacyClients" flags="">
<type class="javax.baja.sys.BDynamicEnum"/>
<description>
Slot for the &lt;code&gt;supportLegacyClients&lt;/code&gt; property.&#xa; Whether or not to respond to legacy N4 Fox clients using the old Fox hello&#xa; exchange.
</description>
<tag name="@since">Niagara 4.8</tag>
<tag name="@see">#getSupportLegacyClients</tag>
<tag name="@see">#setSupportLegacyClients</tag>
</property>

<action name="resetAllConnections" flags="c">
<return>
<type class="void"/>
</return>
<description>
Slot for the &lt;code&gt;resetAllConnections&lt;/code&gt; action.
</description>
<tag name="@see">#resetAllConnections()</tag>
</action>

<action name="delayStationStarted" flags="h">
<return>
<type class="void"/>
</return>
<description>
Slot for the &lt;code&gt;delayStationStarted&lt;/code&gt; action.
</description>
<tag name="@see">#delayStationStarted()</tag>
</action>

</class>
