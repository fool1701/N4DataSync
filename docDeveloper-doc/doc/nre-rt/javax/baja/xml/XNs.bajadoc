<class module="nre" runtimeProfile="rt" qualifiedName="javax.baja.xml.XNs" name="XNs" packageName="javax.baja.xml" public="true" final="true">
<description>
XNs models an XML namespace.  XNs are usually created as&#xa; attributes on XElems using the &lt;code&gt;XElem.defineNs()&lt;/code&gt;&#xa; and &lt;code&gt;XElem.defineDefaultNs()&lt;/code&gt; methods.  Two&#xa; XNs instances are equal if they have the same uri.
</description>
<tag name="@author">Brian Frank on 6 Apr 02</tag>
<tag name="@since">Baja 1.0</tag>
<extends>
<type class="java.lang.Object"/>
</extends>
<!-- javax.baja.xml.XNs(java.lang.String, java.lang.String) -->
<constructor name="XNs" public="true">
<parameter name="prefix">
<type class="java.lang.String"/>
<description>
namespace prefix
</description>
</parameter>
<parameter name="uri">
<type class="java.lang.String"/>
<description>
namespace uri
</description>
</parameter>
<description>
Create a new XNs instance with the specified&#xa; prefix and uri.
</description>
</constructor>

<!-- javax.baja.xml.XNs.isDefault() -->
<method name="isDefault"  public="true">
<description>
Return if this a default XNs namespace which has a prefix of &#x22;&#x22;.
</description>
<return>
<type class="boolean"/>
<description>
true if this is a default namespace
</description>
</return>
</method>

<!-- javax.baja.xml.XNs.prefix() -->
<method name="prefix"  public="true" final="true">
<description>
Get the prefix used to tag elements with this namespace.&#xa; If this is the default namespace then return &#x22;&#x22;.
</description>
<return>
<type class="java.lang.String"/>
<description>
the prefix of this namespace
</description>
</return>
</method>

<!-- javax.baja.xml.XNs.uri() -->
<method name="uri"  public="true" final="true">
<description>
Get the uri which defines a universally unique namespace.
</description>
<return>
<type class="java.lang.String"/>
<description>
the uri of this namespace
</description>
</return>
</method>

