<class module="nre" runtimeProfile="rt" qualifiedName="javax.baja.xml.XParserEvent" name="XParserEvent" packageName="javax.baja.xml" public="true" interface="true" abstract="true" category="interface">
<description>
An event generated by XParserEventGenerator.
</description>
<tag name="@author">Hugh Eaves</tag>
<tag name="@since">Niagara 4.6</tag>
<!-- javax.baja.xml.XParserEvent.getContent() -->
<method name="getContent"  public="true" abstract="true">
<description>
Gets the content. This is either an XElem for ELEM_START / ELEM_END events,&#xa; or XText for TEXT events.
</description>
<return>
<type class="javax.baja.xml.XContent"/>
<description>
the content
</description>
</return>
</method>

<!-- javax.baja.xml.XParserEvent.getLocation() -->
<method name="getLocation"  public="true" abstract="true">
<description>
Gets the location of this event, which is the opening / closing element&#xa; in the case ELEM_START / ELEM_END events, or the parent element, in&#xa; the case of a TEXT event.
</description>
<return>
<type class="javax.baja.xml.XElemLocation"/>
<description/>
</return>
</method>

<!-- javax.baja.xml.XParserEvent.getEventId() -->
<method name="getEventId"  public="true" abstract="true">
<description>
Gets the event id. The event id is one of: XParser.EOF, XParser.ELEM_START.&#xa; XParser.ELEM_END, XParser.TEXT.
</description>
<return>
<type class="int"/>
<description>
the event id
</description>
</return>
</method>

<!-- javax.baja.xml.XParserEvent.isSelfClosing() -->
<method name="isSelfClosing"  public="true" abstract="true">
<description>
If the current element is self closing (i.e. ends with &lt;code&gt;&amp;#x22;/&amp;gt;&amp;#x22;&lt;/code&gt;), and the event&#xa; is either ELEM_START / ELEM_END, then this method returns true
</description>
<return>
<type class="boolean"/>
<description>
true, if is self closing
</description>
</return>
</method>

</class>
