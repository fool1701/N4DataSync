<class module="nre" runtimeProfile="rt" qualifiedName="javax.baja.nre.security.HsmManager" name="HsmManager" packageName="javax.baja.nre.security" public="true" interface="true" abstract="true" category="interface">
<description>
Provides a standard way to determine if there is an hsm implementation available.
</description>
<tag name="@since">Niagara 4.6</tag>
<!-- javax.baja.nre.security.HsmManager.hasHsmEngine() -->
<method name="hasHsmEngine"  public="true" abstract="true">
<description>
Returns true if there is a loadable hsm engine implementation
</description>
<return>
<type class="boolean"/>
</return>
</method>

<!-- javax.baja.nre.security.HsmManager.getHsmEngineClassName() -->
<method name="getHsmEngineClassName"  public="true" abstract="true">
<description>
Returns the name of the class registered as an hsm implementation or null of not configured.&#xa; NOTE: This method *may* return a classname even if hasHsmEngine return false.
</description>
<return>
<type class="java.lang.String"/>
</return>
</method>

<!-- javax.baja.nre.security.HsmManager.getProperties() -->
<method name="getProperties"  public="true" abstract="true">
<description>
Returns the list of properties associated with the registered hsm engine or null of not present.
</description>
<return>
<parameterizedType class="java.util.Map">
<args>
<type class="java.lang.String"/>
<type class="java.lang.String"/>
</args>
</parameterizedType>
</return>
</method>

<!-- javax.baja.nre.security.HsmManager.getHsmType() -->
<method name="getHsmType"  public="true" default="true">
<description>
Returns the &#x22;type&#x22; of hsm chipset available or &#x22;none&#x22; of not present.
</description>
<return>
<type class="java.lang.String"/>
</return>
</method>

<!-- javax.baja.nre.security.HsmManager.hasHsm() -->
<method name="hasHsm"  public="true" default="true">
<description>
Simple helper method that returns true if the &#x22;type&#x22; is not &#x22;none&#x22;.
</description>
<return>
<type class="boolean"/>
</return>
</method>

