<constructor name="LinkedQueue" public="true">
<description/>
</constructor>

<!-- com.tridium.ndriver.util.LinkedQueue.dequeue() -->
<method name="dequeue"  public="true" synchronized="true">
<description>
Read the oldest object from the queue.  If none available wait for the next&#xa; object.
</description>
<return>
<type class="com.tridium.ndriver.util.LinkedQueue$ILinkable"/>
</return>
</method>

<!-- com.tridium.ndriver.util.LinkedQueue.dequeue(int) -->
<method name="dequeue"  public="true" synchronized="true">
<description>
Read the oldest object from the queue.  If none available wait the&#xa; specified time for the next object. If still none available return null.
</description>
<parameter name="timeout">
<type class="int"/>
</parameter>
<return>
<type class="com.tridium.ndriver.util.LinkedQueue$ILinkable"/>
</return>
</method>

<!-- com.tridium.ndriver.util.LinkedQueue.enqueue(com.tridium.ndriver.util.LinkedQueue.ILinkable) -->
<method name="enqueue"  public="true" synchronized="true">
<description>
Adds an linkable to the end of the LinkedQueue.
</description>
<parameter name="value">
<type class="com.tridium.ndriver.util.LinkedQueue$ILinkable"/>
<description>
Linkable to append to the end of the queue.
</description>
</parameter>
<return>
<type class="void"/>
</return>
</method>

<!-- com.tridium.ndriver.util.LinkedQueue.clear() -->
<method name="clear"  public="true" synchronized="true">
<description>
Remove all the enqueued entries.
</description>
<return>
<type class="void"/>
</return>
