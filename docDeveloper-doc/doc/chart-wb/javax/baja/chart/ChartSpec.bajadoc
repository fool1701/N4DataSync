<class module="chart" runtimeProfile="wb" qualifiedName="javax.baja.chart.ChartSpec" name="ChartSpec" packageName="javax.baja.chart" public="true">
<description>
A ChartSpec associates a data set with its axes.
</description>
<tag name="@author">John Sublett</tag>
<tag name="@creation">28 Jun 2004</tag>
<tag name="@version">$Revision: 5$ $Date: 12/21/04 2:20:31 PM EST$</tag>
<tag name="@since">Baja 1.0</tag>
<extends>
<type class="java.lang.Object"/>
</extends>
<!-- javax.baja.chart.ChartSpec(javax.baja.chart.Series) -->
<constructor name="ChartSpec" public="true">
<parameter name="series">
<type class="javax.baja.chart.Series"/>
<description>
The series to be rendered in the chart.
</description>
</parameter>
<description>
Create a ChartSpec with the specified series.&#xa; This constructor is intended for charts with no axes.
</description>
</constructor>

<!-- javax.baja.chart.ChartSpec(javax.baja.chart.Series, javax.baja.chart.BAxis, javax.baja.chart.BAxis) -->
<constructor name="ChartSpec" public="true">
<parameter name="series">
<type class="javax.baja.chart.Series"/>
<description>
The series to be rendered in the chart.
</description>
</parameter>
<parameter name="xaxis">
<type class="javax.baja.chart.BAxis"/>
<description>
The axis used to translate values in column 0 of the series.&#xa;   The axis translates from value space to display space.
</description>
</parameter>
<parameter name="yaxis">
<type class="javax.baja.chart.BAxis"/>
<description>
The axis used to translate values in column 1 of the series.&#xa;   The axis translates from value space to display space.
</description>
</parameter>
<description>
Create a ChartSpec with the specified series and axes.
</description>
</constructor>

<!-- javax.baja.chart.ChartSpec.getSeries() -->
<method name="getSeries"  public="true">
<description>
Get the series for this spec.
</description>
<return>
<type class="javax.baja.chart.Series"/>
</return>
</method>

<!-- javax.baja.chart.ChartSpec.getXAxis() -->
<method name="getXAxis"  public="true">
<description>
Get the axis that is used to translate values in column 0 of the&#xa; series for this spec.
</description>
<return>
<type class="javax.baja.chart.BAxis"/>
</return>
</method>

<!-- javax.baja.chart.ChartSpec.getYAxis() -->
<method name="getYAxis"  public="true">
<description>
Get the axis that is used to translate values in column 1 of the&#xa; series for this spec.
</description>
<return>
<type class="javax.baja.chart.BAxis"/>
</return>
</method>

<!-- javax.baja.chart.ChartSpec.setModel(javax.baja.chart.ChartModel) -->
<method name="setModel"  public="true">
<description>
Set the model that this spec belongs to.  This is called&#xa; by ChartModel when a spec is added.
</description>
<parameter name="model">
<type class="javax.baja.chart.ChartModel"/>
</parameter>
<return>
<type class="void"/>
</return>
</method>

<!-- javax.baja.chart.ChartSpec.getModel() -->
<method name="getModel"  public="true">
<description>
Get the model that this spec belongs to.  A ChartSpec can only be part&#xa; of one model.
</description>
<return>
<type class="javax.baja.chart.ChartModel"/>
</return>
</method>

</class>
