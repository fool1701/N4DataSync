<!-- javax.baja.user.BUserPrototype.updateUserFromPrototype(javax.baja.user.BUser, java.util.Map&lt;java.lang.String, javax.baja.sys.BValue&gt;) -->
<method name="updateUserFromPrototype"  public="true">
<description>
Convenience method for &lt;code&gt;updateUserFromPrototype(user, values, false)&lt;/code&gt;
</description>
<parameter name="user">
<type class="javax.baja.user.BUser"/>
<description>
User to update from prototype
</description>
</parameter>
<parameter name="values">
<parameterizedType class="java.util.Map">
<args>
<type class="java.lang.String"/>
<type class="javax.baja.sys.BValue"/>
</args>
</parameterizedType>
<description>
Map of name-value pairs to take precedence over prototype properties
</description>
</parameter>
<return>
<type class="void"/>
</return>
</method>

<!-- javax.baja.user.BUserPrototype.updateUserFromPrototype(javax.baja.user.BUser, java.util.Map&lt;java.lang.String, javax.baja.sys.BValue&gt;, boolean) -->
<method name="updateUserFromPrototype"  public="true">
<description>
Update a &lt;code&gt;BUser&lt;/code&gt; with values from this prototype. If a property exists in&#xa; the &lt;code&gt;values&lt;/code&gt; map, the user&#x27;s property will be set from it, if not, it will&#xa; be set from this &lt;code&gt;BUserPrototype&lt;/code&gt;&#x27;s property. Properties that are overridable&#xa; and overridden on the user (Flags.USER_DEFINED_1 set) will remain set to the current value.&#xa; Properties that are not overridable will be set to readonly.
</description>
<parameter name="user">
<type class="javax.baja.user.BUser"/>
<description>
User to update from prototype
</description>
</parameter>
<parameter name="values">
<parameterizedType class="java.util.Map">
<args>
<type class="java.lang.String"/>
<type class="javax.baja.sys.BValue"/>
</args>
</parameterizedType>
<description>
Map of name-value pairs to take precedence over prototype properties
</description>
</parameter>
<parameter name="clearOverrides">
<type class="boolean"/>
<description>
Clear any overridden properties if true
</description>
</parameter>
<return>
<type class="void"/>
</return>
</method>

