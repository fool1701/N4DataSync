<class module="baja" runtimeProfile="rt" qualifiedName="javax.baja.units.UnitDifferentialConverter" name="UnitDifferentialConverter" packageName="javax.baja.units" public="true" final="true">
<description>
UnitDifferentialConverter provides a mapping&#xa; between the differential and absolute versions&#xa; of a unit.&#xa; E.g. temperature and temperature differential&#xa; &#xa; Accessible via UnitDifferentialConvert.INSTANCE
</description>
<tag name="@author">Melanie Coggan</tag>
<tag name="@creation">May 2, 2013</tag>
<extends>
<type class="java.lang.Object"/>
</extends>
<!-- javax.baja.units.UnitDifferentialConverter.getDifferential(javax.baja.units.BUnit) -->
<method name="getDifferential"  public="true">
<description>
Returns the differential unit corresponding&#xa; to the provided unit. If the unit is already&#xa; a differential or the differential is the &#xa; same, returns the provided unit.
</description>
<parameter name="absolute">
<type class="javax.baja.units.BUnit"/>
</parameter>
<return>
<type class="javax.baja.units.BUnit"/>
</return>
</method>

<!-- javax.baja.units.UnitDifferentialConverter.getAbsolute(javax.baja.units.BUnit) -->
<method name="getAbsolute"  public="true">
<description>
Returns the absolute unit corresponding&#xa; to the provided unit. If the unit is already&#xa; absolute or the absolute is the &#xa; same, returns the provided unit.
</description>
<parameter name="differential">
<type class="javax.baja.units.BUnit"/>
</parameter>
<return>
<type class="javax.baja.units.BUnit"/>
</return>
</method>

