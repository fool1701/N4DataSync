<!-- javax.baja.security.BPasswordAuthenticator.checkPassword(javax.baja.security.BPassword, javax.baja.sys.Context) -->
<method name="checkPassword"  public="true">
<description>
Check if the specified password meets password requirements. Checks will&#xa; only be performed on reversible passwords, calls with an irreversible password&#xa; will skip chacks and return successfully.
</description>
<tag name="@since">Niagara 4.10</tag>
<parameter name="newPassword">
<type class="javax.baja.security.BPassword"/>
<description>
the new password to check, must be reversible
</description>
</parameter>
<parameter name="context">
<type class="javax.baja.sys.Context"/>
<description>
the context
</description>
</parameter>
<return>
<type class="void"/>
</return>
</method>

<!-- javax.baja.security.BPasswordAuthenticator.checkPassword(javax.baja.user.BUser, javax.baja.authn.BPasswordAuthenticationScheme, com.tridium.user.BUserPasswordConfiguration, javax.baja.security.BPassword, javax.baja.sys.Context) -->
<method name="checkPassword"  public="true" static="true">
<description>
Check if the specified password meets password requirements. Checks will&#xa; only be performed on reversible passwords, calls with an irreversible password&#xa; will skip chacks and return successfully.
</description>
<tag name="@since">Niagara 4.10</tag>
<parameter name="user">
<type class="javax.baja.user.BUser"/>
<description>
the user the password is for, or null if a new user
</description>
</parameter>
<parameter name="scheme">
<type class="javax.baja.authn.BPasswordAuthenticationScheme"/>
<description>
the authentication scheme for the user, or null if not available
</description>
</parameter>
<parameter name="config">
<type class="com.tridium.user.BUserPasswordConfiguration"/>
<description>
the users UserPasswordConfiguration, or null if not available
</description>
</parameter>
<parameter name="newPassword">
<type class="javax.baja.security.BPassword"/>
<description>
the new password to check, must be reversible
</description>
</parameter>
<parameter name="context">
<type class="javax.baja.sys.Context"/>
<description>
the context
</description>
</parameter>
<return>
<type class="void"/>
</return>
</method>

