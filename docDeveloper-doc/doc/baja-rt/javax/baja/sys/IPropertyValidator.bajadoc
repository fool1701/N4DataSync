<!-- javax.baja.sys.IPropertyValidator.validateSet(javax.baja.sys.Validatable, javax.baja.sys.Context) -->
<method name="validateSet"  public="true" abstract="true">
<description>
Validates a pending (possibly batch) property set on a BComplex instance.
</description>
<tag name="@see">CannotValidateException</tag>
<parameter name="validatable">
<type class="javax.baja.sys.Validatable"/>
<description>
The validatable instance which contains the existing&#xa; and proposed state of a BComplex that needs to be validated.
</description>
</parameter>
<parameter name="context">
<type class="javax.baja.sys.Context"/>
<description>
the Context for the pending set operation. Might be null.
</description>
</parameter>
<return>
<type class="void"/>
</return>
</method>

<!-- javax.baja.sys.IPropertyValidator.validateSet(javax.baja.sys.BComplex, javax.baja.sys.Property, javax.baja.sys.BValue, javax.baja.sys.Context) -->
<method name="validateSet"  public="true" default="true">
<description>
Validates a pending property set on a BComplex instance.
</description>
<tag name="@see">CannotValidateException</tag>
<parameter name="instance">
<type class="javax.baja.sys.BComplex"/>
<description>
The BComplex instance for which there is &#xa; a pending set operation requiring validation.
</description>
</parameter>
<parameter name="property">
<type class="javax.baja.sys.Property"/>
<description>
The property on the BComplex instance for&#xa; which there is a pending set operation.
</description>
</parameter>
<parameter name="newValue">
<type class="javax.baja.sys.BValue"/>
<description>
The pending new value for the property on the&#xa; BComplex instance which should be validated prior to commit.
</description>
</parameter>
<parameter name="context">
<type class="javax.baja.sys.Context"/>
<description>
the Context for the pending set operation. Might be null.
</description>
</parameter>
<return>
<type class="void"/>
</return>
</method>

<!-- javax.baja.sys.IPropertyValidator.adjustPendingSetValues(javax.baja.sys.BComplex, javax.baja.sys.Property[], javax.baja.sys.BValue[], javax.baja.sys.Context) -->
<method name="adjustPendingSetValues"  public="true" default="true">
<description>
Callback that provides an opportunity to adjust (and possibly validate) a pending property&#xa; batch set operation on a BComplex instance.
</description>
<tag name="@see">CannotValidateException</tag>
<tag name="@since">Niagara 4.8</tag>
<parameter name="instance">
<type class="javax.baja.sys.BComplex"/>
<description>
The BComplex instance for which there is a pending batch set operation&#xa;                 requiring possible property value adjustment/validation.
</description>
</parameter>
<parameter name="properties">
<type class="javax.baja.sys.Property" dimension="1"/>
<description>
The properties on the BComplex instance for which there is a pending batch&#xa;                   set operation.
</description>
</parameter>
<parameter name="values">
<type class="javax.baja.sys.BValue" dimension="1"/>
<description>
The pending new values for the properties on the BComplex instance which should&#xa;               be adjusted/validated prior to commit. This array argument should be the same&#xa;               size and matching order of the preceding properties argument.
</description>
</parameter>
<parameter name="context">
<type class="javax.baja.sys.Context"/>
<description>
the Context for the pending batch set operation. Might be null.
</description>
</parameter>
<return>
<type class="javax.baja.sys.BValue" dimension="1"/>
<description>
The adjusted/validated values for the properties on the BComplex instance which should&#xa;         be ready to commit. This array result should be the same size and matching order of the&#xa;         preceding properties argument.
</description>
</return>
</method>

<!-- javax.baja.sys.IPropertyValidator.adjustPendingSetValue(javax.baja.sys.BComplex, javax.baja.sys.Property, javax.baja.sys.BValue, javax.baja.sys.Context) -->
