<class module="baja" runtimeProfile="rt" qualifiedName="javax.baja.sys.BatchSetException" name="BatchSetException" packageName="javax.baja.sys" public="true" category="exception">
<description>
A BatchSetException is thrown when a failure occurs during a batch&#xa; &lt;code&gt;set()&lt;/code&gt; operation on a BComplex instance.  It gives information&#xa; about which properties in the batch set failed.
</description>
<tag name="@author">Scott Hoye</tag>
<tag name="@creation">Aug 23, 2013</tag>
<tag name="@version">$Revision: 1$ $Date: 8/23/13 1:57:55 PM EDT$</tag>
<tag name="@since">Niagara 4.0</tag>
<extends>
<type class="javax.baja.sys.BajaRuntimeException"/>
</extends>
<!-- javax.baja.sys.BatchSetException(javax.baja.sys.BComplex, javax.baja.sys.Property[], javax.baja.sys.BValue[], java.lang.Throwable[]) -->
<constructor name="BatchSetException" public="true">
<parameter name="complex">
<type class="javax.baja.sys.BComplex"/>
<description>
- The BComplex instance on which the failed batch set occurred.
</description>
</parameter>
<parameter name="properties">
<type class="javax.baja.sys.Property" dimension="1"/>
<description>
- The properties of the batch set operation that failed to be &#xa; committed on the BComplex instance.
</description>
</parameter>
<parameter name="failedValues">
<type class="javax.baja.sys.BValue" dimension="1"/>
<description>
- The proposed property values that failed to be committed&#xa; on the BComplex instance during the batch set operation.  The size of this array&#xa; should match the properties array argument and the elements should correspond with those&#xa; in the properties array argument.
</description>
</parameter>
<parameter name="causes">
<type class="java.lang.Throwable" dimension="1"/>
<description>
- The causes for the failed property sets on the BComplex instance.&#xa; The size of this array should match the properties array argument and the elements should &#xa; correspond with those in the properties array argument.
</description>
</parameter>
<description>
Constructor to create a BatchSetException indicating that&#xa; the given properties failed to be set on the given BComplex&#xa; instance.
</description>
</constructor>

<!-- javax.baja.sys.BatchSetException(java.lang.String, javax.baja.sys.BComplex, javax.baja.sys.Property[], javax.baja.sys.BValue[], java.lang.Throwable[]) -->
<constructor name="BatchSetException" public="true">
<parameter name="localizedMessage">
<type class="java.lang.String"/>
<description>
- A message for this exception (should be localized for the &#xa; current Locale)
</description>
</parameter>
<parameter name="complex">
<type class="javax.baja.sys.BComplex"/>
<description>
- The BComplex instance on which the failed batch set occurred.
</description>
</parameter>
<parameter name="properties">
<type class="javax.baja.sys.Property" dimension="1"/>
<description>
- The properties of the batch set operation that failed to be &#xa; committed on the BComplex instance.
</description>
</parameter>
<parameter name="failedValues">
<type class="javax.baja.sys.BValue" dimension="1"/>
<description>
- The proposed property values that failed to be committed&#xa; on the BComplex instance during the batch set operation.  The size of this array&#xa; should match the properties array argument and the elements should correspond with those&#xa; in the properties array argument.
</description>
</parameter>
<parameter name="causes">
<type class="java.lang.Throwable" dimension="1"/>
<description>
- The causes for the failed property sets on the BComplex instance.&#xa; The size of this array should match the properties array argument and the elements should &#xa; correspond with those in the properties array argument.
</description>
</parameter>
<description>
Constructor to create a BatchSetException indicating that&#xa; the given properties failed to be set on the given BComplex&#xa; instance.  Also includes an optional localized message for the&#xa; exception.
</description>
</constructor>

<!-- javax.baja.sys.BatchSetException.getComplex() -->
<method name="getComplex"  public="true">
<description>
Returns the BComplex instance for which this BatchSetException indicates&#xa; a failed batch &lt;code&gt;set()&lt;/code&gt; operation.
</description>
<return>
<type class="javax.baja.sys.BComplex"/>
</return>
</method>

<!-- javax.baja.sys.BatchSetException.getFailedProperties() -->
<method name="getFailedProperties"  public="true">
<description>
Returns the properties of the batch &lt;code&gt;set()&lt;/code&gt; operation that failed to be&#xa; committed on the BComplex instance.
</description>
<return>
<type class="javax.baja.sys.Property" dimension="1"/>
</return>
</method>

<!-- javax.baja.sys.BatchSetException.getFailedValues() -->
<method name="getFailedValues"  public="true">
<description>
Returns the proposed property values that failed to be committed&#xa; on the BComplex instance during the batch &lt;code&gt;set()&lt;/code&gt; operation.
</description>
<return>
<type class="javax.baja.sys.BValue" dimension="1"/>
</return>
</method>

<!-- javax.baja.sys.BatchSetException.getFailureCauses() -->
<method name="getFailureCauses"  public="true">
<description>
Returns the causes for the failed property sets on the BComplex instance.
</description>
<return>
<type class="java.lang.Throwable" dimension="1"/>
</return>
</method>

</class>
</bajadoc>
