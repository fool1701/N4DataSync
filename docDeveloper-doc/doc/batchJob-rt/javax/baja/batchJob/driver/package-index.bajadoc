<package module="batchJob" runtimeProfile="rt" name="javax.baja.batchJob.driver">
<description/>
<class packageName="javax.baja.batchJob.driver" name="BDeviceJobPrototype"/>
<class packageName="javax.baja.batchJob.driver" name="BDeviceJobStep"><description>Base class for a job step that is to be run for each device&#xa; in a BForEachDeviceStage&#xa; &#xa; The step creates a BDeviceStepDetails for each device it &#xa; processes during stage execution.</description></class>
<class packageName="javax.baja.batchJob.driver" name="BDeviceNetworkJob"><description>Batch job that processes a given list of devices in a single BDeviceNetwork</description></class>
<class packageName="javax.baja.batchJob.driver" name="BDeviceStepDetails"><description>Stores the statistics for a BDeviceJobStep execution with a single device</description></class>
<class packageName="javax.baja.batchJob.driver" name="BForEachDeviceStage"><description>Job stage for a BDeviceNetworkJob which iterates through each of the&#xa; job&#x27;s devices, and executes a sequence of steps for each device.</description></class>
<class packageName="javax.baja.batchJob.driver" name="BNetworkBatchAgent"><description>Utility for identifying devices in a network that allow batch job&#xa; processing, for getting device identifiers, and for finding&#xa; devices by id.</description></class>
<class packageName="javax.baja.batchJob.driver" name="BNetworkJobStage"><description>Job stage for a BDeviceNetworkJob that executes a sequence &#xa; of steps exactly once.</description></class>
<class packageName="javax.baja.batchJob.driver" name="BNetworkJobStep"><description>Base class for a job step that is to be run once during a &#xa; in a BNetworkJobStage&#xa; &#xa; The step creates a single BNetworkStepDetails object</description></class>
<class packageName="javax.baja.batchJob.driver" name="BNetworkStepDetails"><description>Stores the statistics for a BNetworkJobStep execution</description></class>
<class packageName="javax.baja.batchJob.driver" name="DeviceNetworkJobOp"/>
<class packageName="javax.baja.batchJob.driver" name="BIDeviceNetworkJobSummary" category="interface"><description>Summary information about a completed BDeviceNetworkJob</description></class>
<class packageName="javax.baja.batchJob.driver" name="BIDeviceStepSummary" category="interface"><description>Summary information about a completed BDeviceJobStep</description></class>
<class packageName="javax.baja.batchJob.driver" name="BINetworkStepSummary" category="interface"><description>Summary information about a completed BNetworkJobStep</description></class>
</package>
