<module name="control" runtimeProfile="rt" bajaVersion="0" vendor="Tridium" vendorVersion="4.13.3.48">
<description>Niagara Control Module</description>
<package name="javax.baja.control"><description>&lt;p&gt;This package provides classes for modeling control points.</description></package>
