<!-- javax.baja.ui.spell.SpellingError.getInvalidWord() -->
<method name="getInvalidWord"  public="true" abstract="true">
<description>
Returns the misspelt word.
</description>
<return>
<type class="java.lang.String"/>
</return>
</method>

<!-- javax.baja.ui.spell.SpellingError.getPosition() -->
<method name="getPosition"  public="true" abstract="true">
<description>
Returns the start character index of the mispelt word in &#xa; the context of the text body passed into the SpellChecker.
</description>
<return>
<type class="int"/>
</return>
</method>

<!-- javax.baja.ui.spell.SpellingError.getSuggestions() -->
<method name="getSuggestions"  public="true" abstract="true">
<description>
Returns the list of suggested Word objects.
</description>
<return>
<type class="java.lang.String" dimension="1"/>
</return>
</method>

