<description>
TreeSubject is a Subject with additional Tree specific data.
</description>
<tag name="@author">Brian Frank</tag>
<tag name="@creation">3 May 05</tag>
<tag name="@version">$Revision: 1$ $Date: 5/4/05 7:57:45 PM EDT$</tag>
<tag name="@since">Baja 1.0</tag>
<extends>
<type class="javax.baja.ui.Subject"/>
</extends>
<!-- javax.baja.ui.tree.TreeSubject(javax.baja.ui.tree.BTree, javax.baja.ui.tree.TreeNode[]) -->
<constructor name="TreeSubject" public="true">
<parameter name="tree">
<type class="javax.baja.ui.tree.BTree"/>
</parameter>
<parameter name="nodes">
<type class="javax.baja.ui.tree.TreeNode" dimension="1"/>
</parameter>
<description>
Convenience for &lt;code&gt;this(tree, nodes, null)&lt;/code&gt;
</description>
</constructor>

<!-- javax.baja.ui.tree.TreeSubject(javax.baja.ui.tree.BTree, javax.baja.ui.tree.TreeNode[], javax.baja.ui.tree.TreeNode) -->
<constructor name="TreeSubject" public="true">
<parameter name="tree">
<type class="javax.baja.ui.tree.BTree"/>
</parameter>
<parameter name="nodes">
<type class="javax.baja.ui.tree.TreeNode" dimension="1"/>
</parameter>
<parameter name="activeNode">
<type class="javax.baja.ui.tree.TreeNode"/>
</parameter>
<description>
Construct with specified list of tree nodes and active tree node.
</description>
</constructor>

<!-- javax.baja.ui.tree.TreeSubject.getTree() -->
<method name="getTree"  public="true">
<description>
Get the tree associated with this subject.
</description>
<return>
<type class="javax.baja.ui.tree.BTree"/>
</return>
</method>

<!-- javax.baja.ui.tree.TreeSubject.getNodes() -->
<method name="getNodes"  public="true">
<description>
Get all the selected nodes.
</description>
<return>
<type class="javax.baja.ui.tree.TreeNode" dimension="1"/>
</return>
</method>

<!-- javax.baja.ui.tree.TreeSubject.getNode(int) -->
<method name="getNode"  public="true">
<description>
Get the selected TreeNode at the specified index.
</description>
<parameter name="index">
<type class="int"/>
</parameter>
<return>
<type class="javax.baja.ui.tree.TreeNode"/>
</return>
</method>

<!-- javax.baja.ui.tree.TreeSubject.getActiveNode() -->
<method name="getActiveNode"  public="true">
<description>
Get the active TreeNode or null.
</description>
<return>
<type class="javax.baja.ui.tree.TreeNode"/>
</return>
</method>

</class>
