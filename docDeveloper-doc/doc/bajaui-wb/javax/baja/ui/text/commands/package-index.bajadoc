<?xml version="1.0" encoding="UTF-8"?>
<bajadoc version="2.0" createdBy="niagara-baja-doclet-1.0.9" createdAt="03-May-2024" createdOn="883e7f7a9875">
<package module="bajaui" runtimeProfile="wb" name="javax.baja.ui.text.commands">
<description>
&lt;p&gt;&#xa;This package provides the text commands.&#xa;&lt;/p&gt;
</description>
<class packageName="javax.baja.ui.text.commands" name="Backspace"><description>Backspace removes the previous character.</description></class>
<class packageName="javax.baja.ui.text.commands" name="ComposeText"/>
<class packageName="javax.baja.ui.text.commands" name="CutLine"><description>CutLine cuts the line where the caret is currently &#xa; positioned.</description></class>
<class packageName="javax.baja.ui.text.commands" name="Delete"><description>Delete removes the current character.</description></class>
<class packageName="javax.baja.ui.text.commands" name="DeleteWord"><description>DeleteWord deletes all the characters from the caret&#xa; position to the end of the segment.</description></class>
<class packageName="javax.baja.ui.text.commands" name="DocumentEnd"><description>DocumentEnd moves the caret to the last character &#xa; position in the document.</description></class>
<class packageName="javax.baja.ui.text.commands" name="DocumentStart"><description>DocumentStart moves the caret to the first character &#xa; position in the document.</description></class>
<class packageName="javax.baja.ui.text.commands" name="EditCommand"><description>EditCommand is the abstract base class for commands&#xa; which edit the actual text document either through&#xa; an insert, remove, or both.</description></class>
<class packageName="javax.baja.ui.text.commands" name="Find"><description>Find displays a dialog to prompt the user for search &#xa; criteria then performs a find next.</description></class>
<class packageName="javax.baja.ui.text.commands" name="FindNext"><description>FindNext finds the next instance of the current search string.</description></class>
<class packageName="javax.baja.ui.text.commands" name="FindPrev"><description>FindPrev finds the previous instance of the current search string.</description></class>
<class packageName="javax.baja.ui.text.commands" name="Goto"><description>Goto prompts for a document location to jump to.</description></class>
<class packageName="javax.baja.ui.text.commands" name="InsertText"><description>InsertText inserts specified text at the caret position.</description></class>
<class packageName="javax.baja.ui.text.commands" name="LineEnd"><description>LineEnd moves the caret to the end of the current line.</description></class>
<class packageName="javax.baja.ui.text.commands" name="LineStart"><description>LineStart moves the caret to the first non-whitepsace&#xa; character on the current line.</description></class>
<class packageName="javax.baja.ui.text.commands" name="MoveCommand"><description>MoveCommand is the abstract base class for command&#xa; which move the cursor.</description></class>
<class packageName="javax.baja.ui.text.commands" name="MoveDown"><description>Move the caret to next line.</description></class>
<class packageName="javax.baja.ui.text.commands" name="MoveLeft"><description>Move the caret to the next character.</description></class>
<class packageName="javax.baja.ui.text.commands" name="MoveRight"><description>Move the caret to the previous character.</description></class>
<class packageName="javax.baja.ui.text.commands" name="MoveUp"><description>Move the caret to the previous line.</description></class>
<class packageName="javax.baja.ui.text.commands" name="PageDown"><description>Move the caret down by several lines based on the current&#xa; number of visible lines.</description></class>
<class packageName="javax.baja.ui.text.commands" name="PageUp"><description>Move the caret up by several lines based on the current&#xa; number of visible lines.</description></class>
<class packageName="javax.baja.ui.text.commands" name="ReloadMacros"><description>ReloadMacros displays a dialog to prompt the user for search &#xa; criteria then performs a ReloadMacros next.</description></class>
<class packageName="javax.baja.ui.text.commands" name="RemoveText"><description>RemoveText removes the currently selected text.</description></class>
<class packageName="javax.baja.ui.text.commands" name="Replace"><description>Replace does a search and replace.</description></class>
<class packageName="javax.baja.ui.text.commands" name="ReplaceAll"><description>ReplaceAll</description></class>
<class packageName="javax.baja.ui.text.commands" name="SelectAll"><description>SelectAll displays a dialog to prompt the user for search &#xa; criteria then performs a SelectAll next.</description></class>
<class packageName="javax.baja.ui.text.commands" name="TabBack"><description>TabBack removes a tab from the beginning of the &#xa; selected lines.</description></class>
<class packageName="javax.baja.ui.text.commands" name="TabForward"><description>TabForward has two modes of operation.</description></class>
<class packageName="javax.baja.ui.text.commands" name="TextEditorCommand"><description>TextEditorCommand provides a common base class for&#xa; the standard BTextEditor commands.</description></class>
<class packageName="javax.baja.ui.text.commands" name="ToggleSlashSlash"><description>ToggleSlashSlash toggles the C++/Java &#x22;//&#x22; comment&#xa; on and off from the beginning of selected lines.</description></class>
<class packageName="javax.baja.ui.text.commands" name="WordLeft"><description>Move the caret to the start position of the current word,&#xa; or if the current position not in a word segment, then go&#xa; to the start position of the previous word.</description></class>
<class packageName="javax.baja.ui.text.commands" name="WordRight"><description>Move the caret to the ending position of the current word, &#xa; or if the current position is not inside a word segment &#xa; then the go to the ending position of the next word.</description></class>
<class packageName="javax.baja.ui.text.commands" name="WordWrap"><description>WordWrap takes the selected text removes all occurances&#xa; of single line breaks, then inserts new line breaks so &#xa; that each line is no longer than 80 characters.</description></class>
</package>
</bajadoc>
