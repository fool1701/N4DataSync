<type class="javax.baja.ui.BWidget"/>
</parameter>
<parameter name="properties">
<type class="javax.baja.ui.px.PxProperty" dimension="1"/>
</parameter>
<parameter name="layers">
<type class="javax.baja.ui.px.PxLayer" dimension="1"/>
</parameter>
