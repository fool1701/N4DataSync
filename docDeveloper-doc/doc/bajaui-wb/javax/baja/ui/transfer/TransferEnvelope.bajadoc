</description>
<return>
<type class="javax.baja.ui.transfer.TransferFormat" dimension="1"/>
</return>
</method>

<!-- javax.baja.ui.transfer.TransferEnvelope.getData(javax.baja.ui.transfer.TransferFormat) -->
<method name="getData"  public="true" final="true">
<description>
