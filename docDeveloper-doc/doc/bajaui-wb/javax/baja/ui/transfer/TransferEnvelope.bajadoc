<type class="java.lang.Object"/>
</extends>
<!-- javax.baja.ui.transfer.TransferEnvelope.make(java.lang.String) -->
<method name="make"  public="true" static="true">
<description>
Make a TransferEnvelope instance for string data.
</description>
<parameter name="string">
<type class="java.lang.String"/>
</parameter>
<return>
<type class="javax.baja.ui.transfer.TransferEnvelope"/>
</return>
</method>

<!-- javax.baja.ui.transfer.TransferEnvelope.make(javax.baja.space.Mark) -->
<method name="make"  public="true" static="true">
<description>
Make a TransferEnvelope instance for a Mark.
</description>
<parameter name="mark">
<type class="javax.baja.space.Mark"/>
</parameter>
<return>
<type class="javax.baja.ui.transfer.TransferEnvelope"/>
</return>
