<class module="bajaui" runtimeProfile="wb" qualifiedName="javax.baja.ui.style.IStylable" name="IStylable" packageName="javax.baja.ui.style" public="true" interface="true" abstract="true" category="interface">
<description>
Interface for a component that can be styled using an external theme&#xa; configuration file.
</description>
<tag name="@author">Logan Byam</tag>
<tag name="@creation">12 Jun 11</tag>
<tag name="@version">$Revision: 2$ $Date: 7/12/11 11:12:41 AM EDT$</tag>
<tag name="@since">Niagara 3.7</tag>
<!-- javax.baja.ui.style.IStylable.getStyleId() -->
<method name="getStyleId"  public="true" abstract="true">
<description>
Returns the style ID for this component. This allows a theme configuration&#xa; file to refer to this component by ID, similar to a CSS &#xa; &lt;code&gt;#myStyleId&lt;/code&gt; selector.
</description>
<return>
<type class="java.lang.String"/>
<description>
the style ID for this component
</description>
</return>
</method>

<!-- javax.baja.ui.style.IStylable.setStyleId(java.lang.String) -->
<method name="setStyleId"  public="true" abstract="true">
<description>
Sets the style ID for this component.
</description>
<parameter name="styleId">
<type class="java.lang.String"/>
<description>
the style ID to set
</description>
</parameter>
<return>
<type class="void"/>
</return>
</method>

<!-- javax.baja.ui.style.IStylable.getStyleClasses() -->
<method name="getStyleClasses"  public="true" abstract="true">
<description>
Returns the list of style classes (whitespace separated) that this&#xa; component currently has. This allows a theme configuration file to assign&#xa; styles to components given a certain class, similar to a CSS&#xa; &lt;code&gt;.myStyleClass&lt;/code&gt; selector.
</description>
<return>
<type class="java.lang.String"/>
<description>
a whitespace-separated list of style classes assigned to this&#xa; component
</description>
</return>
</method>

<!-- javax.baja.ui.style.IStylable.setStyleClasses(java.lang.String) -->
<method name="setStyleClasses"  public="true" abstract="true">
<description>
Sets the list of style classes assigned to this component.
</description>
<parameter name="styleClasses">
<type class="java.lang.String"/>
<description>
a whitespace-separated list of style classes
</description>
</parameter>
<return>
<type class="void"/>
</return>
</method>

<!-- javax.baja.ui.style.IStylable.getStyleSelector() -->
<method name="getStyleSelector"  public="true" abstract="true">
<description>
Returns a selector indicating where in a configuration file to find style&#xa; information for this component. This property is most analogous to an&#xa; HTML tag: e.g., a &lt;code&gt;BLabel&lt;/code&gt; will return &lt;code&gt;&#x22;label&#x22;&lt;/code&gt;,&#xa; a &lt;code&gt;BTable&lt;/code&gt; will return &lt;code&gt;&#x22;table&#x22;&lt;/code&gt;, etc.&#xa; &#xa; &lt;p&gt;There is no corresponding &lt;code&gt;setStyleSelector&lt;/code&gt; method - if&#xa; you need different styling for a certain subset of components, it is best &#xa; to assign style classes to these components instead.
</description>
<return>
<type class="java.lang.String"/>
<description>
the style selector for this component
</description>
</return>
</method>

</class>
</bajadoc>
