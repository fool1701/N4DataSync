<class module="orion" runtimeProfile="rt" qualifiedName="com.tridium.orion.ISchemaUpgrader" name="ISchemaUpgrader" packageName="com.tridium.orion" public="true" interface="true" abstract="true" category="interface">
<description>
Implementing classes upgrade the schema version of a single&#xa; Orion application between specific versions
</description>
<tag name="@author">Matt Boon</tag>
<tag name="@creation">4 Sep 2008</tag>
<tag name="@version">$Revision$ $Date$</tag>
