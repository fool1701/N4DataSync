<extends>
<type class="com.tridium.tls.point.BTls350StringProxyExt"/>
</extends>
<implements>
<type class="com.tridium.tls.messages.TlsMessageConst"/>
</implements>
<property name="minimumPollInterval" flags="">
<type class="javax.baja.sys.BRelTime"/>
<description>
Slot for the &lt;code&gt;minimumPollInterval&lt;/code&gt; property.&#xa; minimumPollInterval for this object
</description>
<tag name="@see">#getMinimumPollInterval</tag>
<tag name="@see">#setMinimumPollInterval</tag>
</property>

<property name="fuelManagementTable" flags="">
<type class="com.tridium.tls.datatypes.BFuelManagementTable"/>
<description>
Slot for the &lt;code&gt;fuelManagementTable&lt;/code&gt; property.&#xa; array of BFuelManagementEntry(s) retrieved to date       -- reporting old entries)
</description>
<tag name="@see">#getFuelManagementTable</tag>
<tag name="@see">#setFuelManagementTable</tag>
</property>

<property name="maxOldEntriesSaved" flags="">
<type class="int"/>
<description>
Slot for the &lt;code&gt;maxOldEntriesSaved&lt;/code&gt; property.&#xa; support maxOldEntriesSaved just for historical purposes&#xa; provide reasonable default (16 entries)&#xa; be careful not to make this number too high for too long as&#xa; it can consume a lot of system memory
</description>
<tag name="@see">#getMaxOldEntriesSaved</tag>
<tag name="@see">#setMaxOldEntriesSaved</tag>
</property>

<action name="purgeHashtable" flags="h">
<return>
<type class="void"/>
</return>
<description>
Slot for the &lt;code&gt;purgeHashtable&lt;/code&gt; action.&#xa; flag process to run that purges old Hashtable values&#xa; let it run in sycn with poll which actually builds the fMHashtable
</description>
<tag name="@see">#purgeHashtable()</tag>
