<extends>
<type class="javax.baja.hx.Event"/>
</extends>
<!-- javax.baja.hx.Command(javax.baja.hx.BHxView) -->
<constructor name="Command" public="true">
<parameter name="view">
<type class="javax.baja.hx.BHxView"/>
</parameter>
<description>
Create a new Event with this id.
</description>
</constructor>

<!-- javax.baja.hx.Command.getDisplayName(javax.baja.hx.HxOp) -->
<method name="getDisplayName"  public="true">
<description>
Return the display name for this event. &#xa; Returns id by default.
</description>
<parameter name="op">
<type class="javax.baja.hx.HxOp"/>
</parameter>
<return>
<type class="java.lang.String"/>
</return>
</method>

