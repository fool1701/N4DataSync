<type class="javax.baja.hx.HxOp"/>
</parameter>
<return>
<type class="void"/>
</return>
<throws>
<type class="java.lang.Exception"/>
</throws>
</method>

<!-- javax.baja.hx.Command.redirect(javax.baja.hx.HxOp, java.lang.String) -->
<method name="redirect"  public="true">
