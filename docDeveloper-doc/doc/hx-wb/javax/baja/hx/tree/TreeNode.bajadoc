<!-- javax.baja.hx.tree.TreeNode(javax.baja.hx.tree.TreeModel, javax.baja.hx.tree.TreeNode) -->
<constructor name="TreeNode" public="true">
<parameter name="model">
<type class="javax.baja.hx.tree.TreeModel"/>
</parameter>
<parameter name="parent">
<type class="javax.baja.hx.tree.TreeNode"/>
</parameter>
