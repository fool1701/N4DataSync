</return>
</method>

<!-- javax.baja.hx.mgr.HxMgrModel.valueToIndex(java.lang.String) -->
<method name="valueToIndex"  public="true">
<description>
Get the index for the row with the specified value.
</description>
<parameter name="rowValue">
<type class="java.lang.String"/>
</parameter>
<return>
<type class="int"/>
</return>
</method>

<!-- javax.baja.hx.mgr.HxMgrModel.valueToComponent(java.lang.String) -->
<method name="valueToComponent"  public="true">
<description>
Get the row component for the specified value.&#xa; The default implementation is getRows()[valueToIndex(rowValue)].
</description>
<parameter name="rowValue">
<type class="java.lang.String"/>
</parameter>
<return>
<type class="javax.baja.sys.BComponent"/>
</return>
</method>

<!-- javax.baja.hx.mgr.HxMgrModel.getColumns() -->
<method name="getColumns"  public="true" final="true">
<description>
Get the columns to display for the database table.  These&#xa; columns are initialized from the makeColumns() method.
</description>
<return>
<type class="javax.baja.hx.mgr.HxMgrColumn" dimension="1"/>
</return>
</method>

<!-- javax.baja.hx.mgr.HxMgrModel.getColumn(int) -->
<method name="getColumn"  public="true" final="true">
<description>
Get the column at the specified index.
</description>
<parameter name="index">
<type class="int"/>
</parameter>
<return>
<type class="javax.baja.hx.mgr.HxMgrColumn"/>
</return>
