<type class="javax.baja.hx.HxOp"/>
</parameter>
<return>
<type class="java.lang.String"/>
</return>
</method>

<!-- javax.baja.hx.table.view.TableViewModel.getRowObject(int, int, javax.baja.hx.HxOp) -->
<method name="getRowObject"  public="true" abstract="true">
<description>
Return the object that this row represents.
</description>
<parameter name="group">
<type class="int"/>
</parameter>
<parameter name="row">
<type class="int"/>
</parameter>
<parameter name="op">
<type class="javax.baja.hx.HxOp"/>
</parameter>
<return>
<type class="java.lang.Object"/>
</return>
</method>

<!-- javax.baja.hx.table.view.TableViewModel.toRowId(int, int, javax.baja.hx.HxOp) -->
<method name="toRowId"  public="true">
<description>
Get the row id to use for this row. Row ids must be unique &#xa; for each row.  Default returns &lt;code&gt;Integer.toString(row)&lt;/code&gt;.
</description>
<parameter name="group">
<type class="int"/>
</parameter>
<parameter name="row">
<type class="int"/>
</parameter>
<parameter name="op">
<type class="javax.baja.hx.HxOp"/>
</parameter>
<return>
<type class="java.lang.String"/>
</return>
</method>

<!-- javax.baja.hx.table.view.TableViewModel.fromRowId(java.lang.String, javax.baja.hx.HxOp) -->
<method name="fromRowId"  public="true">
<description>
Get the row value that corresponds to this row id. Default&#xa; returns &lt;code&gt;getRowObject(1, Integer.parseInt(id))&lt;/code&gt;.
</description>
<parameter name="id">
<type class="java.lang.String"/>
</parameter>
<parameter name="op">
<type class="javax.baja.hx.HxOp"/>
</parameter>
<return>
<type class="java.lang.Object"/>
</return>
</method>

<!-- javax.baja.hx.table.view.TableViewModel.getOrd(int, int, int, javax.baja.hx.HxOp) -->
<method name="getOrd"  public="true">
<description>
Get the ord for the object in the given column. Return&#xa; BOrd.NULL for no hyperlinks. Default is BOrd.NULL.
</description>
<parameter name="group">
<type class="int"/>
</parameter>
<parameter name="row">
<type class="int"/>
</parameter>
<parameter name="col">
<type class="int"/>
</parameter>
<parameter name="op">
<type class="javax.baja.hx.HxOp"/>
</parameter>
<return>
<type class="javax.baja.naming.BOrd"/>
</return>
</method>

<!-- javax.baja.hx.table.view.TableViewModel.makeCommands(javax.baja.hx.HxOp) -->
<method name="makeCommands"  public="true">
<description>
Return the commands used for this table. This method is called &#xa; to install the event handlers on &lt;code&gt;BTableView.write()&lt;/code&gt;.
</description>
<parameter name="op">
<type class="javax.baja.hx.HxOp"/>
</parameter>
<return>
<type class="javax.baja.hx.Command" dimension="1"/>
</return>
</method>

<!-- javax.baja.hx.table.view.TableViewModel.getSelectedRowIds(javax.baja.hx.HxOp) -->
<method name="getSelectedRowIds"  public="true">
<description>
Return an array of row ids from the selected rows in the client.&#xa; Returns an empty array if no rows are selected.
</description>
<parameter name="op">
<type class="javax.baja.hx.HxOp"/>
</parameter>
<return>
<type class="java.lang.String" dimension="1"/>
</return>
</method>

