<class module="hx" runtimeProfile="wb" qualifiedName="javax.baja.hx.Event" name="Event" packageName="javax.baja.hx" public="true" abstract="true">
<description>
Event encapsulates a event that is fired from the browser&#xa; and handled on the station.
</description>
<tag name="@author">Andy Frank</tag>
<tag name="@creation">27 Jun 05</tag>
<tag name="@version">$Revision: 2$ $Date: 9/29/09 9:51:01 AM EDT$</tag>
<tag name="@since">Baja 1.0</tag>
<extends>
<type class="java.lang.Object"/>
</extends>
<!-- javax.baja.hx.Event(javax.baja.hx.BHxView) -->
<constructor name="Event" public="true">
<parameter name="view">
<type class="javax.baja.hx.BHxView"/>
</parameter>
<description>
Create a new Event with this id.
</description>
</constructor>

<!-- javax.baja.hx.Event.getView() -->
<method name="getView"  public="true">
<description>
Get the view for this event.
</description>
<return>
<type class="javax.baja.hx.BHxView"/>
</return>
</method>

<!-- javax.baja.hx.Event.getId() -->
<method name="getId"  public="true" final="true">
