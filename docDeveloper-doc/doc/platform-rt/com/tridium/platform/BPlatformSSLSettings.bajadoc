<?xml version="1.0" encoding="UTF-8"?>
<bajadoc version="2.0" createdBy="niagara-baja-doclet-1.0.9" createdAt="03-May-2024" createdOn="883e7f7a9875">
<class module="platform" runtimeProfile="rt" qualifiedName="com.tridium.platform.BPlatformSSLSettings" name="BPlatformSSLSettings" packageName="com.tridium.platform" public="true">
<description>
Attributes for platform SSL Settings
</description>
<extends>
<type class="javax.baja.sys.BComponent"/>
</extends>
<property name="sslLicensed" flags="tr">
<type class="boolean"/>
<description>
Slot for the &lt;code&gt;sslLicensed&lt;/code&gt; property.&#xa; is ssl a licensed feature
</description>
<tag name="@see">#getSslLicensed</tag>
<tag name="@see">#setSslLicensed</tag>
</property>

<property name="sslEnabled" flags="tr">
<type class="boolean"/>
<description>
Slot for the &lt;code&gt;sslEnabled&lt;/code&gt; property.&#xa; is ssl turned on
</description>
<tag name="@see">#getSslEnabled</tag>
<tag name="@see">#setSslEnabled</tag>
</property>

<property name="sslOnly" flags="tr">
<type class="boolean"/>
<description>
Slot for the &lt;code&gt;sslOnly&lt;/code&gt; property.&#xa; should non local host connections be redirected to the secure port
</description>
<tag name="@see">#getSslOnly</tag>
<tag name="@see">#setSslOnly</tag>
</property>

<property name="sslEnabledStateReadonly" flags="tr">
<type class="boolean"/>
<description>
Slot for the &lt;code&gt;sslEnabledStateReadonly&lt;/code&gt; property.&#xa; can the enabled state of SSL be modified
</description>
<tag name="@see">#getSslEnabledStateReadonly</tag>
<tag name="@see">#setSslEnabledStateReadonly</tag>
</property>

<property name="sslPort" flags="tr">
<type class="int"/>
<description>
Slot for the &lt;code&gt;sslPort&lt;/code&gt; property.&#xa; should non local host connections be redirected to the secure port
</description>
<tag name="@see">#getSslPort</tag>
<tag name="@see">#setSslPort</tag>
</property>

<property name="keyAlias" flags="tr">
<type class="java.lang.String"/>
<description>
Slot for the &lt;code&gt;keyAlias&lt;/code&gt; property.&#xa; alias for server certificate
</description>
<tag name="@see">#getKeyAlias</tag>
<tag name="@see">#setKeyAlias</tag>
</property>

<property name="keyPassphrase" flags="tr">
<type class="javax.baja.security.BPassword"/>
<description>
Slot for the &lt;code&gt;keyPassphrase&lt;/code&gt; property.&#xa; password for server certificate
</description>
<tag name="@since">Niagara 4.13</tag>
<tag name="@see">#getKeyPassphrase</tag>
<tag name="@see">#setKeyPassphrase</tag>
</property>

<property name="sslAlgType" flags="tr">
<type class="javax.baja.security.crypto.BSslTlsEnum"/>
<description>
Slot for the &lt;code&gt;sslAlgType&lt;/code&gt; property.&#xa; SSL Alg type, e.g. TLSV1, TLSV1_1, TLSV1_2
</description>
<tag name="@see">#getSslAlgType</tag>
<tag name="@see">#setSslAlgType</tag>
</property>

<property name="tlsCipherSuiteGroup" flags="tr">
<type class="javax.baja.security.crypto.BTlsCipherSuiteGroup"/>
<description>
Slot for the &lt;code&gt;tlsCipherSuiteGroup&lt;/code&gt; property.&#xa; the cipher suite group to use for tls connections
</description>
<tag name="@see">#getTlsCipherSuiteGroup</tag>
<tag name="@see">#setTlsCipherSuiteGroup</tag>
</property>

<property name="fipsMode" flags="tr">
<type class="boolean"/>
<description>
Slot for the &lt;code&gt;fipsMode&lt;/code&gt; property.&#xa; is true if the server side is running in fips mode
</description>
<tag name="@see">#getFipsMode</tag>
<tag name="@see">#setFipsMode</tag>
</property>

<property name="useExtendedMasterSecret" flags="tr">
<type class="java.lang.String"/>
<description>
Slot for the &lt;code&gt;useExtendedMasterSecret&lt;/code&gt; property.&#xa; should the extended master secret extension be used for TLS connections
</description>
<tag name="@see">#getUseExtendedMasterSecret</tag>
<tag name="@see">#setUseExtendedMasterSecret</tag>
</property>

</class>
</bajadoc>
