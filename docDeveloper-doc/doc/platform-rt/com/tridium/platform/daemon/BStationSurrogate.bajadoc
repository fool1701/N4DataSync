<class module="platform" runtimeProfile="rt" qualifiedName="com.tridium.platform.daemon.BStationSurrogate" name="BStationSurrogate" packageName="com.tridium.platform.daemon" public="true">
<description>
Represents a station running on a host that&#x27;s connected to a BDaemonConsole&#xa; plugin
</description>
<tag name="@author">Matt Boon</tag>
<tag name="@creation">03 Jun 02</tag>
<tag name="@version">$Revision: 56$ $Date: 11/23/09 2:28:09 PM EST$</tag>
<tag name="@since">Baja 1.0</tag>
<extends>
<type class="com.tridium.platform.daemon.BAppSurrogate"/>
</extends>
<property name="foxPort" flags="rt">
<type class="int"/>
<description>
Slot for the &lt;code&gt;foxPort&lt;/code&gt; property.&#xa; IP port on which the station&#x27;s fox service is listening, or PORT_UNKNOWN&#xa; if unknown or fox not running
</description>
<tag name="@see">#getFoxPort</tag>
<tag name="@see">#setFoxPort</tag>
</property>

<property name="foxsPort" flags="rt">
<type class="int"/>
<description>
Slot for the &lt;code&gt;foxsPort&lt;/code&gt; property.&#xa; IP port on which the station&#x27;s secure fox service is listening, or PORT_UNKNOWN&#xa; if unknown or foxs not running
</description>
<tag name="@see">#getFoxsPort</tag>
<tag name="@see">#setFoxsPort</tag>
</property>

<property name="httpPort" flags="rt">
<type class="int"/>
<description>
Slot for the &lt;code&gt;httpPort&lt;/code&gt; property.&#xa; IP port on which the station&#x27;s web service is listening, or PORT_UNKNOWN&#xa; if unknown or web service not running
</description>
<tag name="@see">#getHttpPort</tag>
<tag name="@see">#setHttpPort</tag>
</property>

<property name="httpsPort" flags="rt">
<type class="int"/>
<description>
Slot for the &lt;code&gt;httpsPort&lt;/code&gt; property.&#xa; IP port on which the station&#x27;s secure web service is listening, or PORT_UNKNOWN&#xa; if unknown or secure web service not running
</description>
<tag name="@see">#getHttpsPort</tag>
<tag name="@see">#setHttpsPort</tag>
</property>

<property name="foxOrd" flags="rt">
<type class="javax.baja.naming.BOrd"/>
<description>
Slot for the &lt;code&gt;foxOrd&lt;/code&gt; property.&#xa; ord that can be followed to open the station in workbench
</description>
<tag name="@see">#getFoxOrd</tag>
<tag name="@see">#setFoxOrd</tag>
</property>

<property name="foxsOrd" flags="rt">
<type class="javax.baja.naming.BOrd"/>
<description>
Slot for the &lt;code&gt;foxsOrd&lt;/code&gt; property.&#xa; ord that can be followed to open the station securely in workbench
</description>
<tag name="@see">#getFoxsOrd</tag>
<tag name="@see">#setFoxsOrd</tag>
</property>

</class>
