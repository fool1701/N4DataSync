<class module="bacnet" runtimeProfile="rt" qualifiedName="com.tridium.bacnet.history.BBacnetTrendLogAlarmSourceExt" name="BBacnetTrendLogAlarmSourceExt" packageName="com.tridium.bacnet.history" public="true">
