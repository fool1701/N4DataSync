<!-- javax.baja.neql.LikeExpression.evalBoolean(javax.baja.tag.Entity, javax.baja.sys.Context) -->
<method name="evalBoolean"  protected="true" final="true">
<annotation><type class="java.lang.Override"/>
</annotation>
<description>
Returns true if this expression&#x27;s regex matches the single value or any of the values coming&#xa; from the left expression.
</description>
<tag name="@since">Niagara 4.9</tag>
<parameter name="entity">
<type class="javax.baja.tag.Entity"/>
<description>
entity on which to evaluate the left expression
</description>
</parameter>
<parameter name="context">
<type class="javax.baja.sys.Context"/>
<description>
context to use while evaluating the left expression
</description>
</parameter>
<return>
<type class="boolean"/>
<description>
true if this expression&#x27;s regex matches the single value or any of the values coming&#xa;   from the left expression
</description>
</return>
</method>

