<class module="niagaraVirtual" runtimeProfile="rt" qualifiedName="com.tridium.nv.BSlotInfo" name="BSlotInfo" packageName="com.tridium.nv" public="true" final="true">
<description>
Niagara Virtual Slot Information&#xa; &lt;p&gt;&#xa; This Struct is used to Cache Slot Information for Niagara Virtuals
</description>
<tag name="@author">gjohnson</tag>
<tag name="@creation">5 Jan 2010</tag>
<tag name="@version">1</tag>
<tag name="@since">Niagara 3.7</tag>
<extends>
<type class="javax.baja.sys.BStruct"/>
</extends>
<property name="slotName" flags="">
<type class="java.lang.String"/>
<description>
Slot for the &lt;code&gt;slotName&lt;/code&gt; property.&#xa; The original Server side Slot name
</description>
<tag name="@see">#getSlotName</tag>
<tag name="@see">#setSlotName</tag>
</property>

<property name="slotDisplayName" flags="">
<type class="javax.baja.util.BFormat"/>
<description>
Slot for the &lt;code&gt;slotDisplayName&lt;/code&gt; property.&#xa; The display name from the Server
</description>
<tag name="@see">#getSlotDisplayName</tag>
<tag name="@see">#setSlotDisplayName</tag>
</property>

<property name="slotOrd" flags="">
<type class="javax.baja.naming.BOrd"/>
<description>
Slot for the &lt;code&gt;slotOrd&lt;/code&gt; property.&#xa; The full Slot (and possibly Virtual as well) Path on the Server
</description>
<tag name="@see">#getSlotOrd</tag>
<tag name="@see">#setSlotOrd</tag>
</property>

<property name="isFrozen" flags="">
<type class="boolean"/>
<description>
Slot for the &lt;code&gt;isFrozen&lt;/code&gt; property.&#xa; Was the Slot originally frozen?
</description>
<tag name="@see">#getIsFrozen</tag>
<tag name="@see">#setIsFrozen</tag>
</property>

<property name="isComponent" flags="">
<type class="boolean"/>
<description>
Slot for the &lt;code&gt;isComponent&lt;/code&gt; property.&#xa; Was the Slot originally a Component? (need this is case we can&#x27;t resolve Type on Client)
</description>
<tag name="@see">#getIsComponent</tag>
<tag name="@see">#setIsComponent</tag>
</property>

<property name="isWritable" flags="">
<type class="boolean"/>
<description>
Slot for the &lt;code&gt;isWritable&lt;/code&gt; property.&#xa; If not a Component, is the Property writable? (3.4 had read-only Properties)
</description>
<tag name="@see">#getIsWritable</tag>
<tag name="@see">#setIsWritable</tag>
</property>

<property name="slotType" flags="">
<type class="com.tridium.nv.BSlotType"/>
<description>
Slot for the &lt;code&gt;slotType&lt;/code&gt; property.&#xa; The original Slot Type (Property, Action or Topic)
</description>
<tag name="@see">#getSlotType</tag>
<tag name="@see">#setSlotType</tag>
</property>

<property name="isNiagaraVirtual" flags="">
<type class="boolean"/>
<description>
Slot for the &lt;code&gt;isNiagaraVirtual&lt;/code&gt; property.&#xa; Is this Slot another Niagara Virtual? Used for Niagara Virtual to Niagara Virtual case
</description>
<tag name="@see">#getIsNiagaraVirtual</tag>
<tag name="@see">#setIsNiagaraVirtual</tag>
</property>

<property name="typeSpec" flags="">
<type class="java.lang.String"/>
<description>
Slot for the &lt;code&gt;typeSpec&lt;/code&gt; property.&#xa; The original Type Specification of the Slot
</description>
<tag name="@see">#getTypeSpec</tag>
<tag name="@see">#setTypeSpec</tag>
</property>

<property name="facets" flags="">
<type class="javax.baja.sys.BFacets"/>
<description>
Slot for the &lt;code&gt;facets&lt;/code&gt; property.&#xa; The original Slot Facets (only set with Server info if Slot not a Component of any type)
</description>
<tag name="@see">#getFacets</tag>
<tag name="@see">#setFacets</tag>
</property>

<property name="slotFlags" flags="">
<type class="int"/>
<description>
Slot for the &lt;code&gt;slotFlags&lt;/code&gt; property.&#xa; The original Slot Flags (only set with Server info if Slot not a Component of any type)
</description>
<tag name="@see">#getSlotFlags</tag>
<tag name="@see">#setSlotFlags</tag>
</property>

<property name="returnTypeSpec" flags="">
<type class="java.lang.String"/>
<description>
Slot for the &lt;code&gt;returnTypeSpec&lt;/code&gt; property.&#xa; The return type (if mapped to an Action)
</description>
<tag name="@see">#getReturnTypeSpec</tag>
<tag name="@see">#setReturnTypeSpec</tag>
</property>

<property name="encodedValue" flags="">
<type class="java.lang.String"/>
<description>
Slot for the &lt;code&gt;encodedValue&lt;/code&gt; property.&#xa; Optional encoded value. Available since Niagara 4.4
</description>
<tag name="@see">#getEncodedValue</tag>
<tag name="@see">#setEncodedValue</tag>
</property>

<property name="slotIcon" flags="">
<type class="javax.baja.sys.BIcon"/>
<description>
Slot for the &lt;code&gt;slotIcon&lt;/code&gt; property.&#xa; Optional icon for component types.
</description>
<tag name="@since">Niagara 4.7</tag>
<tag name="@see">#getSlotIcon</tag>
<tag name="@see">#setSlotIcon</tag>
</property>

</class>
